<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.time-from-start" path="/org/gnome/shell/extensions/time-from-start/">
    <key name="time-format" type="s">
      <default>"default"</default>
    </key>
    <key name="system-user" type="s">
      <default>"system"</default>
    </key>
    <key name="timer-time" type="s">
      <default>"Timer: 0h 0m"</default>
    </key>
    <key name="timer-minutes" type="u">
      <default>0</default>
    </key>
    <key name="timer-hours" type="u">
      <default>0</default>
    </key>
    <key name="timer-stop-minutes" type="u">
      <default>0</default>
    </key>
    <key name="without-downtime" type="b">
      <default>false</default>
    </key>
    <key name="timer-enabled" type="b">
      <default>false</default>
    </key>
    <key name="play-sound" type="b">
      <default>false</default>
    </key>
    <key name="sound-file-map" type="a{ss}">
      <default>{'soundName': 'string.ogg', 'soundPath': '/usr/share/sounds/gnome/default/alerts/string.ogg'}</default>
    </key>
  </schema>
</schemalist>
